frr defaults datacenter
hostname n-router-02
no ipv6 forwarding
!
interface eth1
 ip address 32.43.3.138/30
exit
!
interface eth2
 ip address 166.194.136.13/30
exit
!
router bgp 6380
 bgp router-id 166.194.136.13
 no bgp log-neighbor-changes
 no bgp default show-hostname
 no bgp default show-nexthop-hostname
 no bgp deterministic-med
 no bgp network import-check
 timers bgp 60 180
 neighbor 32.43.3.137 remote-as 7018
 neighbor 32.43.3.137 ebgp-multihop 2
 neighbor 32.43.3.137 timers 2 6
 neighbor 32.43.3.137 timers connect 120
 neighbor 166.194.136.14 remote-as 7091
 neighbor 166.194.136.14 ebgp-multihop 2
 neighbor 166.194.136.14 timers 2 6
 neighbor 166.194.136.14 timers connect 120
 !
 address-family ipv4 unicast
    redistribute connected
 exit-address-family
exit
!
end