frr version 8.4_git
frr defaults traditional
hostname bgp-router-07
no ipv6 forwarding
!
interface eth0
 ip address 13.0.7.2/30
 description bgp-router-05
exit
!
interface eth1
 ip address 14.0.10.1/30
 description bgp-router-10
exit
!
router bgp 14007
 bgp router-id 14.0.0.7
 neighbor 13.0.7.1 remote-as 13005
 neighbor 13.0.7.1 timers 2 6
 neighbor 14.0.10.2 remote-as 15010
 neighbor 14.0.10.2 timers 2 6
 !
 address-family ipv4 unicast
  redistribute connected
  neighbor 13.0.7.1 route-map ALLOW_PREFIX in
  neighbor 13.0.7.1 route-map ALLOW_PREFIX out
  neighbor 14.0.10.2 route-map ALLOW_PREFIX in
  neighbor 14.0.10.2 route-map ALLOW_PREFIX out
 exit-address-family
exit
!
route-map ALLOW_PREFIX permit 10
exit
!
end