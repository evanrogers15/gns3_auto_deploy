frr version 8.4_git
frr defaults traditional
hostname bgp-router-10
no ipv6 forwarding
!
ip route 0.0.0.0/0 192.168.1.1
!
interface eth0
 ip address 14.0.10.2/30
 description bgp-router-07
exit
!
interface eth1
 ip address 14.1.10.2/30
 description bgp-router-08
exit
!
interface eth5
 ip address 172.16.2.1/24
 description bgp-test-mp-1
exit
!
router bgp 15010
 bgp router-id 15.0.0.10
 neighbor 14.0.10.1 remote-as 14007
 neighbor 14.0.10.1 timers 2 6
 neighbor 14.1.10.1 remote-as 14008
 neighbor 14.1.10.1 timers 2 6
 !
 address-family ipv4 unicast
  redistribute connected
  neighbor 14.0.10.1 route-map ALLOW_PREFIX in
  neighbor 14.0.10.1 route-map ALLOW_PREFIX out
  neighbor 14.1.10.1 route-map ALLOW_PREFIX in
  neighbor 14.1.10.1 route-map ALLOW_PREFIX out
 exit-address-family
exit
!
route-map ALLOW_PREFIX permit 10
exit
!
end