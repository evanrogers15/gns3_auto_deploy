frr defaults datacenter
hostname core_bgp_chicago
no ipv6 forwarding
!
interface eth0
 ip address 4.69.209.82/30
exit
!
interface eth1
 ip address 12.66.48.1/30
exit
!
router bgp 7018
 bgp router-id 12.66.48.1
 no bgp log-neighbor-changes
 no bgp default show-hostname
 no bgp default show-nexthop-hostname
 no bgp deterministic-med
 no bgp network import-check
 timers bgp 60 180
 neighbor 4.69.209.81 remote-as 33566
 neighbor 4.69.209.81 ebgp-multihop 2
 neighbor 4.69.209.81 timers 2 6
 neighbor 4.69.209.81 timers connect 120
 neighbor 12.66.48.2 remote-as 27345
 neighbor 12.66.48.2 ebgp-multihop 2
 neighbor 12.66.48.2 timers 2 6
 neighbor 12.66.48.2 timers connect 120
 !
 address-family ipv4 unicast
   redistribute connected
 exit-address-family
exit
!
end