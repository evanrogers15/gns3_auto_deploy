frr defaults datacenter
hostname isp_memphis
no ipv6 forwarding
!
interface eth0
 ip address 63.171.84.6/30
exit
!
interface eth2
 ip address 172.16.3.1/24
 exit
!
router bgp 32068
 bgp router-id 63.171.84.6
 no bgp log-neighbor-changes
 no bgp default show-hostname
 no bgp default show-nexthop-hostname
 no bgp deterministic-med
 no bgp network import-check
 timers bgp 60 180
 neighbor 63.171.84.5 remote-as 32068
 neighbor 63.171.84.5 ebgp-multihop 2
 neighbor 63.171.84.5 timers 2 6
 neighbor 63.171.84.5 timers connect 120
 !
 address-family ipv4 unicast
   redistribute connected
 exit-address-family
exit
!
end