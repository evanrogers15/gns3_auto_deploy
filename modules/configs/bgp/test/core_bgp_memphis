frr defaults datacenter
hostname core_bgp_memphis
no ipv6 forwarding
!
interface eth0
 ip address 67.222.240.2/30
exit
!
interface eth1
 ip address 63.171.84.1/30
exit
!
interface eth2
 ip address 63.171.84.5/30
exit
!
router bgp 32068
 bgp router-id 63.171.84.1
 no bgp log-neighbor-changes
 no bgp default show-hostname
 no bgp default show-nexthop-hostname
 no bgp deterministic-med
 no bgp network import-check
 timers bgp 60 180
 neighbor 67.222.240.1 remote-as 27345
 neighbor 67.222.240.1 ebgp-multihop 2
 neighbor 67.222.240.1 timers 2 6
 neighbor 67.222.240.1 timers connect 120
 neighbor 63.171.84.2 remote-as 33667
 neighbor 63.171.84.2 ebgp-multihop 2
 neighbor 63.171.84.2 timers 2 6
 neighbor 63.171.84.2 timers connect 120
 neighbor 63.171.84.6 remote-as 32068
 neighbor 63.171.84.6 ebgp-multihop 2
 neighbor 63.171.84.6 timers 2 6
 neighbor 63.171.84.6 timers connect 120
 !
 address-family ipv4 unicast
   redistribute connected
 exit-address-family
exit
!
end