frr defaults datacenter
hostname core_bgp_newyork
no ipv6 forwarding
!
ip route 0.0.0.0/0 192.168.1.1
!
interface eth0
 description internet_uplink
 ip address 192.168.1.3/24
exit
!
interface eth1
 ip address 4.69.209.81/30
exit
!
router bgp 33566
 bgp router-id 4.69.209.81
 no bgp log-neighbor-changes
 no bgp default show-hostname
 no bgp default show-nexthop-hostname
 no bgp deterministic-med
 no bgp network import-check
 timers bgp 60 180
 neighbor 4.69.209.82 remote-as 7018
 neighbor 4.69.209.82 ebgp-multihop 2
 neighbor 4.69.209.82 timers 2 6
 neighbor 4.69.209.82 timers connect 120
 !
 address-family ipv4 unicast
  network 0.0.0.0/0
 exit-address-family
exit
!
end