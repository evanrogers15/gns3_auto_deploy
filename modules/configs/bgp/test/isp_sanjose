frr defaults datacenter
hostname isp_sanjose
no ipv6 forwarding
!
interface eth0
 ip address 23.71.26.2/30
exit
!
interface eth2
 ip address 172.16.2.1/24
 exit
!
router bgp 33667
 bgp router-id 23.71.26.2
 no bgp log-neighbor-changes
 no bgp default show-hostname
 no bgp default show-nexthop-hostname
 no bgp deterministic-med
 no bgp network import-check
 timers bgp 60 180
 neighbor 23.71.26.1 remote-as 33667
 neighbor 23.71.26.1 ebgp-multihop 2
 neighbor 23.71.26.1 timers 2 6
 neighbor 23.71.26.1 timers connect 120
 !
 address-family ipv4 unicast
   redistribute connected
 exit-address-family
exit
!
end