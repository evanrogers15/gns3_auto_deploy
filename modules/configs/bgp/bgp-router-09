frr version 8.4_git
frr defaults traditional
hostname bgp-router-09
no ipv6 forwarding
!
interface eth0
 ip address 13.1.9.2/30
 description bgp-router-06
exit
!
interface eth1
 ip address 14.2.11.1/30
 description bgp-router-11
exit
!
router bgp 14009
 bgp router-id 14.0.0.9
 neighbor 13.1.9.1 remote-as 13006
 neighbor 13.1.9.1 timers 2 6
 neighbor 14.2.11.2 remote-as 15011
 neighbor 14.2.11.2 timers 2 6
 !
 address-family ipv4 unicast
  redistribute connected
  neighbor 13.1.9.1 route-map ALLOW_PREFIX in
  neighbor 13.1.9.1 route-map ALLOW_PREFIX out
  neighbor 14.2.11.2 route-map ALLOW_PREFIX in
  neighbor 14.2.11.2 route-map ALLOW_PREFIX out
 exit-address-family
exit
!
route-map ALLOW_PREFIX permit 10
exit
!
end