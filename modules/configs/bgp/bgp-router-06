frr version 8.4_git
frr defaults traditional
hostname bgp-router-06
no ipv6 forwarding
!
ip route 0.0.0.0/0 192.168.1.1
!
interface eth0
 ip address 12.1.6.2/30
 description bgp-router-03
exit
!
interface eth1
 ip address 12.2.6.2/30
 description bgp-router-04
exit
!
interface eth2
 ip address 13.1.8.1/30
 description bgp-router-08
exit
!
interface eth3
 ip address 13.1.9.1/30
 description bgp-router-09
exit
!
router bgp 13006
 bgp router-id 13.0.0.6
 neighbor 12.1.6.1 remote-as 12003
 neighbor 12.1.6.1 timers 2 6
 neighbor 12.2.6.1 remote-as 12004
 neighbor 12.2.6.1 timers 2 6
 neighbor 13.1.8.2 remote-as 14008
 neighbor 13.1.8.2 timers 2 6
 neighbor 13.1.9.2 remote-as 14009
 neighbor 13.1.9.2 timers 2 6
 !
 address-family ipv4 unicast
  redistribute connected
  neighbor 12.1.6.1 route-map ALLOW_PREFIX in
  neighbor 12.1.6.1 route-map ALLOW_PREFIX out
  neighbor 12.2.6.1 route-map ALLOW_PREFIX in
  neighbor 12.2.6.1 route-map ALLOW_PREFIX out
  neighbor 13.1.8.2 route-map ALLOW_PREFIX in
  neighbor 13.1.8.2 route-map ALLOW_PREFIX out
  neighbor 13.1.9.2 route-map ALLOW_PREFIX in
  neighbor 13.1.9.2 route-map ALLOW_PREFIX out
 exit-address-family
exit
!
route-map ALLOW_PREFIX permit 10
exit
!
end